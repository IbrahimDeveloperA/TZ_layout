<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:endColor="#FFB700"
                android:startColor="#F1D180" />
            <corners android:radius="12dp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <gradient
                android:endColor="#0D000000"
                android:startColor="#0D000000" />

            <corners android:radius="12dp" />
        </shape>
    </item>
</layer-list>
